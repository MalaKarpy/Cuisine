<h1>Restaurants for $cuisine.getType()</h1>
  <ul>
    #foreach ($restaurant in $restaurants)
      <li>$restaurant.getName()</li>
      <form action="/restaurants/$restaurant.getRestaurantId()/delete" method="post">
         <button type="submit" class="btn btn-success">Delete</button>
      </form>
    #end
  </ul>
